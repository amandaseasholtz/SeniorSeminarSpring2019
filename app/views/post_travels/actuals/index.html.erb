<p id="notice"><%= notice %></p>

<h1>Actuals</h1>

<table>
  <thead>
    <tr>
      <th>Expense</th>
      <th>Cost</th>
      <th>Payment information</th>
      <th>Post travel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actuals.each do |actual| %>
      <tr>
        <td><%= actual.expense %></td>
        <td><%= actual.cost %></td>
        <td><%= actual.payment_information %></td>
        <td><%= actual.post_travel %></td>
        <td><%= link_to 'Show', actual %></td>
        <td><%= link_to 'Edit', edit_actual_path(actual) %></td>
        <td><%= link_to 'Destroy', actual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actual', new_actual_path %>
